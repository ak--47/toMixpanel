{
  "name": "to-mixpanel",
  "version": "2.0",
  "description": "ETL for data into Mixpanel from many sources",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prune": "rm -rf ./savedData/* ./logs/*",
    "pub": "npm publish",
    "amplitude": "npm run prune && nodemon --ignore ./savedData --inspect --max_old_space_size=8192 ./index.js ./dev/configAMP.json",
    "ga360": "npm run prune && node --inspect --max_old_space_size=8192 ./index.js ./dev/configGA360.json",
    "csv": "npm run prune && nodemon --ignore ./savedData --inspect --max_old_space_size=8192 ./index.js ./dev/configCSV.json",
    "mixpanel": "npm run prune && nodemon --ignore ./savedData --inspect --max_old_space_size=8192 ./index.js ./dev/configMP.json | tee -a ./logs/mp-log.txt",
    "dev": "npm run prune && nodemon --ignore ./savedData --inspect --max_old_space_size=8192 ./index.js ./dev/configDevCSV.json | tee -a ./logs/mp-log.txt"
  },
  "bin": {
    "to-mixpanel": "./index.js"
  },
  "type": "module",
  "keywords": [
    "mixpanel",
    "google analytics",
    "ga360",
    "csv",
    "events",
    "tracking",
    "amplitude",
    "analytics"
  ],
  "author": "AK",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^5.15.6",
    "adm-zip": "^0.5.6",
    "ak-tools": "^1.0.32",
    "ansi-colors": "^4.1.3",
    "axios": "^0.26.0",
    "cli-progress": "^3.12.0",
    "dayjs": "^1.11.7",
    "dotenv": "^10.0.0",
    "inquirer": "^9.1.4",
    "is-gzip": "^2.0.0",
    "jsonschema": "^1.4.0",
    "JSONStream": "^1.3.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mixpanel-import": "^2.2.52",
    "node-fetch": "^3.0.0",
    "node-gzip": "^1.1.2",
    "papaparse": "^5.3.1",
    "split": "^1.0.1"
  }
}
